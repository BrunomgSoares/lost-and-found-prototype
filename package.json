{
  "name": "node-js-skeleton",
  "version": "1.0.0",
  "scripts": {
    "build": "npx tsc",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "lint": "eslint \"{server,apps,libs,test}/**/*.ts\" --fix",
    "start": "node dist/app.js",
    "test": "NODE_ENV=test mocha --recursive --inspect --exit test",
    "test-win": "set NODE_ENV=test&& mocha --recursive --inspect --exit test"
  },
  "dependencies": {
    "body-parser": "1.20.1",
    "celebrate": "15.0.1",
    "express": "4.17.1",
    "helmet": "4.5.0",
    "lodash": "4.17.21",
    "method-override": "3.0.0",
    "moment": "2.29.4",
    "moment-timezone": "0.5.33",
    "mongoose": "5.13.13",
    "request": "2.88.2",
    "request-promise": "4.2.6"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/helmet": "4.0.0",
    "@types/mongodb": "4.0.7",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "chai": "^4.1.2",
    "concurrently": "7.5.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "mocha": "10.0.0",
    "nodemon": "2.0.20",
    "sinon": "10.0.1",
    "typescript": "4.8.4"
  }
}
